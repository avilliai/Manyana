<script>
function shuffleArray(array) {
    for (let i = array.length - 1; i > 0; i--) {
        let j = Math.floor(Math.random() * (i + 1));
        [array[i], array[j]] = [array[j], array[i]];
    }
}

// å°† search å‡½æ•°å®šä¹‰åœ¨å…¨å±€ä½œç”¨åŸŸä¸­
function search(updateDisplay) {
    const loadingInstance = ElLoading.service({ fullscreen: true });
    let name = x.getval('#search');
    
    if (!name.trim()) {
        loadingInstance.close();
        loadAllData(updateDisplay); // ç©ºæœç´¢æ—¶åŠ è½½æ‰€æœ‰æ•°æ®
        return;
    }
    
    x.ajax("extend/ajaxApi.php?act=search", {
        name: name
    }, (data) => {
        loadingInstance.close();
        if (data.code) {
            updateDisplay(data.data || []);
        } else {
            showEmptyState();
        }
    });
}

// å°† loadAllData å‡½æ•°å®šä¹‰åœ¨å…¨å±€ä½œç”¨åŸŸä¸­
function loadAllData(updateDisplay) {
    x.ajax("extend/ajaxApi.php?act=list", {}, (data) => {
        if (data.code) {
            shuffleArray(data.data);
            updateDisplay(data.data || []);
        } else {
            showEmptyState();
        }
    });
}

